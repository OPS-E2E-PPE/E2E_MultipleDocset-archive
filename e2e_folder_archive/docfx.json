{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ],
        "src": ".",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "is_archived": true,
      "is_retired": false,
      "ROBOTS": "NOINDEX,NOFOLLOW",
      "docs_archive": "tool"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "archive-template"
  }
}